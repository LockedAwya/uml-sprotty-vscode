model test

datatype String
datatype Int

interface Test1 {
    title: String
}
interface Test2 {
    title: String
}

interface Test extends Test1 {
    test: String
}

class HasAuthor {
    author: String
}

class Blog {
    title: String
    many posts: Post
}

class Post extends HasAuthor implements Test1 Test2 {
    title: String
    content: String
    many comments: Comment
}

class Comment extends HasAuthor {
    content: String
}